{
  "name": "zml",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:docker": "docker compose --env-file .env.docker.dev -f docker-compose.dev.yml up --build",
    "dev:docker:reset": "docker compose --env-file .env.docker.dev -f docker-compose.dev.yml down -v --remove-orphans",
    "dev:frontend": "next dev --hostname 0.0.0.0 --port 3000",
    "dev:backend": "npm run convex:env:sync && convex dev --env-file .env.docker.dev",
    "convex:env:sync": "node scripts/sync-convex-env.mjs",
    "convex:sync": "npm run convex:env:sync && convex dev --once --env-file .env.docker.dev",
    "convex:codegen": "npm run convex:sync && node scripts/convex-codegen.mjs",
    "test:convex": "npm run convex:sync && node --test scripts/convex-integration.test.mjs",
    "test": "npm run test:convex",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test --project=chromium tests/e2e/smoke.spec.ts",
    "convex:inventory": "node scripts/convex-refactor-inventory.mjs",
    "dev:seed": "node scripts/dev-seed.mjs seed",
    "dev:seed:users": "node scripts/dev-seed.mjs users",
    "dev:seed:simulate": "node scripts/dev-seed.mjs simulate",
    "dev:seed:reset": "node scripts/dev-seed.mjs reset",
    "build": "NODE_ENV=production next build",
    "build:webpack": "NODE_ENV=production next build --webpack",
    "analyze:bundle": "NODE_ENV=production ANALYZE=true next build --webpack",
    "lint": "eslint app components hooks lib proxy.ts next.config.ts",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "check:convex-contract": "npm run convex:inventory && npm run check",
    "scripts:help": "node scripts/scripts-help.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.726.1",
    "@aws-sdk/s3-request-presigner": "3.726.1",
    "@convex-dev/aggregate": "^0.2.1",
    "@convex-dev/better-auth": "^0.10.10",
    "@convex-dev/migrations": "^0.3.1",
    "@convex-dev/sharded-counter": "^0.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@serwist/turbopack": "^9.5.5",
    "@swc/helpers": "^0.5.18",
    "better-auth": "^1.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "esbuild": "^0.25.12",
    "html-to-image": "^1.11.13",
    "jdenticon": "^3.3.0",
    "lucide-react": "^0.563.0",
    "music-metadata": "^11.12.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "serwist": "^9.5.5",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "waveform-data": "^4.5.2",
    "web-push": "^3.6.7",
    "zod": "^4.3.6",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.5.5",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/web-push": "^3.6.4",
    "convex": "^1.31.7",
    "dotenv": "^16.4.7",
    "eslint": "^9.33.0",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^27.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
